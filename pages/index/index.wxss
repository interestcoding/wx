.header{
  height: 32px;
  border-bottom: 1px solid #6E7783;
}
.header .header-item{
  display: inline-block;
  font-weight: bold;
  font-size: 14px;
  padding: 0 10px;
  line-height: 32px;
}
.nav-item{
  height: 32px;
  border-bottom: 1px solid #6E7783;
  display: inline-block;
  width: 25%;
  text-align: center;
  line-height: 32px;
  font-size: 12px;

}
.li{
  font-size: 12px;
  height: 34px;
  padding: 8px 0;
  border-bottom: 1px solid #efefef;
}
.li .li-item{
  width: 25%;
  float: left;
  height: 33px;
}
.li-item .jiaoyi{
  width: 60%;
  float: left;
  text-align: right;
  font-size: 16px;
  height: 20px;
  line-height: 20px;
  font-weight: bold;
}
.li-item .dui{
  width: 40%;
  float: left;
  height: 20px;
  font-size: 10px;
  line-height: 24px;
  text-align: right;
  color: gray;
}
.li-item .jiaoyisuo{
  width: 100%;
  font-size: 10px;
  text-align: right;
  color: gray;
}
.li-item .price{
  height: 20px;
  line-height: 20px;
  font-size: 16px;
}
.li-item .price2{
  font-size: 10px;
}
.bi{
  margin: 5px 5px;
  height:23px;
  text-align: center;
  line-height: 23px;
  background-color: #c03546;
  color: white;
  border-radius: 3px;
}
.bi_green{
  margin: 5px 5px;
  height:23px;
  text-align: center;
  line-height: 23px;
  background-color: #008b00;
  color: white;
  border-radius: 3px;
}
.li-item .liang{
  text-align: center;
  line-height: 33px
}